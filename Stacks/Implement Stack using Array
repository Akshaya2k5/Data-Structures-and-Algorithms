#include <iostream>
using namespace std;

class MyStack {
public:
    int st[100];
    int top = -1;

    void push(int x) {
        if (top < 99) {  
            st[++top] = x;
        } else {
            cout << "Stack Overflow!" << endl;
        }
    }

    int pop() {
        int x = -1;
        if (top > -1) {
            x = st[top];
            top--;
        } else {
            cout << "Stack Underflow!" << endl;
        }
        return x;
    }
};

int main() {
    MyStack s;

    // Push some values
    s.push(10);
    s.push(20);
    s.push(30);

    // Pop and print values
    cout << "Popped: " << s.pop() << endl; // 30
    cout << "Popped: " << s.pop() << endl; // 20
    cout << "Popped: " << s.pop() << endl; // 10
    cout << "Popped: " << s.pop() << endl; // -1 (underflow)

    return 0;
}
